<AnchorPane prefHeight="500.0" prefWidth="300.0" styleClass="WallWork" stylesheets="@../design.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diaryWork.controllers.WeekController">
   <children>
      <Button fx:id="add" layoutX="220.0" layoutY="452.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="35.0" prefWidth="61.0" stylesheets="@../design.css" text="add" textFill="WHITE">
         <styleClass>
            <String fx:value="Button" />
            <String fx:value="Buttonbghover" />
         </styleClass>
         <font>
            <Font name="Franklin Gothic Medium" size="14.0" />
         </font></Button>
      <TextField fx:id="name" layoutX="61.0" layoutY="115.0" prefHeight="26.0" prefWidth="174.0" promptText="work">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="start" layoutX="59.0" layoutY="283.0" prefHeight="26.0" prefWidth="174.0" promptText="Start time">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="end" layoutX="59.0" layoutY="321.0" prefHeight="26.0" prefWidth="174.0" promptText="End time">
