            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <ComboBox fx:id="category" layoutX="62.0" layoutY="73.0" prefHeight="26.0" prefWidth="173.0" promptText="Category">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="important" layoutX="56.0" layoutY="359.0" prefHeight="26.0" prefWidth="174.0" promptText="Priority">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="status" layoutX="56.0" layoutY="400.0" prefHeight="26.0" prefWidth="174.0" promptText="status">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <RadioButton fx:id="sun" layoutX="169.0" layoutY="241.0" mnemonicParsing="false" text="sun" />
      <RadioButton fx:id="tue" layoutX="130.0" layoutY="181.0" mnemonicParsing="false" text="tue" />
      <RadioButton fx:id="fri" layoutX="173.0" layoutY="213.0" mnemonicParsing="false" text="fri" />
      <RadioButton fx:id="thu" layoutX="77.0" layoutY="213.0" mnemonicParsing="false" text="thu" />
