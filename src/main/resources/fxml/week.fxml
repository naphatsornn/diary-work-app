      <RadioButton fx:id="sat" layoutX="78.0" layoutY="241.0" mnemonicParsing="false" text="sat" />
      <RadioButton fx:id="wed" layoutX="189.0" layoutY="181.0" mnemonicParsing="false" text="wed" />
      <Label fx:id="weekly" layoutX="107.0" layoutY="152.0" text="Day of work" />
      <Label layoutX="83.0" layoutY="23.0" text="WORK OF WEEK" textFill="#022f77">
         <font>
            <Font name="Franklin Gothic Heavy" size="18.0" />
         </font>
      </Label>
