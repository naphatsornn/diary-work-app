<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" styleClass="WallWork" stylesheets="@../design.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diaryWork.controllers.ProjectController">
   <children>
      <Button fx:id="add" layoutX="221.0" layoutY="452.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="33.0" prefWidth="58.0" stylesheets="@../design.css" text="add" textFill="WHITE">
         <styleClass>
            <String fx:value="Button" />
            <String fx:value="Buttonbghover" />
         </styleClass>
         <font>
            <Font name="Franklin Gothic Medium" size="14.0" />
         </font></Button>
      <TextField fx:id="name" layoutX="63.0" layoutY="119.0" prefHeight="26.0" prefWidth="174.0" promptText="Job title">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="boss" layoutX="64.0" layoutY="162.0" prefHeight="26.0" prefWidth="174.0" promptText="Project Leader Name">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <ComboBox fx:id="category" layoutX="61.0" layoutY="70.0" prefHeight="26.0" prefWidth="174.0" promptText="Category">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <DatePicker fx:id="dateStart" layoutX="64.0" layoutY="206.0" prefHeight="26.0" prefWidth="174.0" promptText="Date begin" />
      <DatePicker fx:id="dateFinish" layoutX="63.0" layoutY="250.0" prefHeight="26.0" prefWidth="174.0" promptText="Work end date" />
      <ComboBox fx:id="important" layoutX="61.0" layoutY="368.0" prefHeight="26.0" prefWidth="174.0" promptText="Priority">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="status" layoutX="61.0" layoutY="413.0" prefHeight="26.0" prefWidth="174.0" promptText="Status">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <TextField fx:id="start" layoutX="61.0" layoutY="291.0" prefHeight="26.0" prefWidth="174.0" promptText="Start time">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="endTime" layoutX="61.0" layoutY="327.0" prefHeight="26.0" prefWidth="174.0" promptText="End time">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label layoutX="82.0" layoutY="21.0" text="PROJECT WORK" textFill="#022f77">
         <font>
            <Font name="Franklin Gothic Heavy" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
