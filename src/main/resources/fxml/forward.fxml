<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" styleClass="WallWork" stylesheets="@../design.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diaryWork.controllers.ForwardController">
   <children>
      <TextField fx:id="name" layoutX="63.0" layoutY="113.0" prefHeight="26.0" prefWidth="174.0" promptText="work">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
         <font>
            <Font size="13.0" />
         </font></TextField>
      <TextField fx:id="people" layoutX="63.0" layoutY="156.0" prefHeight="26.0" prefWidth="174.0" promptText="Responsible person">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor>
         <font>
            <Font size="13.0" />
         </font></TextField>
      <TextField fx:id="time" layoutX="63.0" layoutY="275.0" prefHeight="26.0" prefWidth="174.0" promptText="Start time">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Button fx:id="add" layoutX="213.0" layoutY="445.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="40.0" prefWidth="66.0" stylesheets="@../design.css" text="add" textFill="WHITE">
         <font>
            <Font name="Franklin Gothic Medium" size="14.0" />
         </font>
         <styleClass>
            <String fx:value="Button" />
            <String fx:value="Buttonbghover" />
         </styleClass>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor></Button>
      <ComboBox fx:id="category" layoutX="64.0" layoutY="68.0" prefHeight="26.0" prefWidth="174.0" promptText="Category">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <DatePicker fx:id="datePicker" layoutX="63.0" layoutY="199.0" prefHeight="26.0" prefWidth="174.0" promptText="Date begin" />
      <ComboBox fx:id="status" layoutX="63.0" layoutY="401.0" prefHeight="26.0" prefWidth="174.0" promptText="Status">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <ComboBox fx:id="important" layoutX="63.0" layoutY="356.0" prefHeight="26.0" prefWidth="174.0" promptText="Priority">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></ComboBox>
      <TextField fx:id="end" layoutX="63.0" layoutY="314.0" prefHeight="26.0" prefWidth="174.0" promptText="End time">
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <DatePicker fx:id="dateEnd" layoutX="63.0" layoutY="237.0" prefHeight="26.0" prefWidth="174.0" promptText="Work end date" />
      <Label layoutX="79.0" layoutY="22.0" text="FORWARD WORK" textFill="#022f77">
         <font>
            <Font name="Franklin Gothic Heavy" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
