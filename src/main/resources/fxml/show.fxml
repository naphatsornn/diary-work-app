<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="diaryWork.controllers.ShowController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../photo/BGShow.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tableList" layoutX="41.0" layoutY="126.0" prefHeight="374.0" prefWidth="320.0" />
      <Label fx:id="categoryName" alignment="TOP_CENTER" layoutX="89.0" layoutY="64.0" prefHeight="46.0" prefWidth="224.0" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS">
         <font>
            <Font name="Baskerville Old Face" size="26.0" />
         </font></Label>
      <Button fx:id="back" layoutX="27.0" layoutY="546.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="35.0" prefWidth="71.0" stylesheets="@../design.css" text="back" textFill="WHITE">
         <font>
            <Font name="Franklin Gothic Heavy" size="14.0" />
         </font>
         <styleClass>
            <String fx:value="ButtonBox" />
            <String fx:value="ButtonHoverBox" />
         </styleClass>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
